{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "typeRoots": ["./node_modules/@types"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "build",
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "test"],
  "extensions": ["ts"],
}